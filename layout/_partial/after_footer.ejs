<% UserConfig.loaded_modules.forEach(function(mod){%>
    <% if (!cache && hexo.debug) { %><!-- module <%- mod -%> Script BEGIN --><%}%>
    <%var module = UserConfig.modules[mod];if (module && module.js) {%><script src="<%- RealPath(module.js) -%>"></script><%}%>
    <%- _partial('../_modules/'+mod,{module: module}) -%>
    <% if (!cache && hexo.debug) { %><!-- module <%- '../_modules/'+mod -%> Script END --><%}%>
<% }); %>
<script type="text/javascript">
$('.ajax_widgets').each(function(){var src=$(this).attr('data-src');if(src)$(this).load('<%-config.root%>widgets/'+src+'.html');});
_js2load.forEach(function(obj){loadjs(obj.src,obj.charset)});
</script>
<% if (!cache && hexo.debug) { %>
